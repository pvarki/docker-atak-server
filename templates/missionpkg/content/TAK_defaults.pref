<?xml version='1.0' standalone='yes'?>
<preferences>
    <preference version="1" name="com.atakmap.app.civ_preferences">
        <!-- Clear content -->
        <entry key="clearingContent" class="class java.lang.Boolean">true</entry>
		<!-- Download datapackages automatically from server -->
		<entry key="deviceProfileEnableOnConnect" class="class java.lang.Boolean">true</entry>
		<!-- Disable default maps from tak.gov -->
		<entry key="eud_api_sync_mapsources" class="class java.lang.Boolean">false</entry> 
		<!-- Scan plugins on ATAK startup -->
		<entry key="atakPluginScanningOnStartup" class="class java.lang.Boolean">true</entry>
		<!-- ATAK display preferencces -->
		<entry key="nav_orientation_right" class="class java.lang.Boolean">true</entry>
		<entry key="self_coord_info_display" class="class java.lang.String">bottom_bar</entry>
		<entry key="monitorServerConnections" class="class java.lang.Boolean">true</entry>
		<entry key="imagery_tab_name" class="class java.lang.String">Maps</entry>
		<entry key="map_scale_visible" class="class java.lang.Boolean">true</entry>
		<entry key="map_scale_rounding" class="class java.lang.Boolean">true</entry>
		<entry key="map_center_designator" class="class java.lang.Boolean">false</entry>
		<entry key="screenViewMapTilt" class="class java.lang.Float">0.0</entry>	
		<!-- ATAK action preferences -->
		<entry key="atakLongPressMap" class="class java.lang.String">nothing</entry>
		<entry key="atakDoubleTapToZoom" class="class java.lang.Boolean">true</entry>
		<entry key="self_coord_action" class="class java.lang.String">nothing</entry>
        <!-- Default Map, disable default maps  -->
        <entry key="MobileLayerSelectionAdapter.selected" class="class java.lang.String">4 Google Hybrid</entry>
        <entry key="rng_meters_display_pref" class="class java.lang.String">2000</entry>
		<entry key="eud_api_sync_mapsources" class="class java.lang.Boolean">false</entry> 
        <!-- Unit for Speed, 0 = imperial 1 = metric -->
        <entry key="speed_unit_pref" class="class java.lang.String">1</entry>
        <!-- Bearing units -->
        <entry key="rab_brg_units_pref" class="class java.lang.String">0</entry>
        <!-- Default setting -->
        <entry key="rab_color_pref" class="class java.lang.String">red</entry>
        <entry key="status_tilt_enabled" class="class java.lang.Boolean">true</entry>
        <entry key="lastViewedLayer.autoselect" class="class java.lang.Boolean">true</entry>
        <entry key="viewshed_prefs_height_above" class="class java.lang.Integer">0</entry>
        <!-- Metric force -->
        <entry key="rab_rng_units_pref" class="class java.lang.String">1</entry>
        <entry key="status_3d_enabled" class="class java.lang.Boolean">true</entry>
		<!-- 0 = 360 degree / 1 = Milrad (NATO) -->
		<entry key="rab_brg_units_pref" class="class java.lang.String">0</entry>
		<!-- Display ETA on Range and Bearing Labels -->
		<entry key="rab_preference_show_eta" class="class java.lang.Boolean">true</entry>
        <!-- Hide Phone number -->
        <entry key="saHasPhoneNumber" class="class java.lang.Boolean">false</entry>
        <entry key="locationUnitType" class="class java.lang.String">a-f-G-U-C</entry>
        <entry key="viewshed_prefs_radius" class="class java.lang.Integer">1000</entry>
        <!-- Height over Sea level force -->
        <entry key="alt_display_pref" class="class java.lang.String">MSL</entry>
		<!-- Disable AGL to show MSL on coordinate display -->
		<entry key="alt_display_agl" class="class java.lang.Boolean">false</entry>
        <!-- MGRS Coordinate force -->
        <entry key="coord_display_pref" class="class java.lang.String">MGRS</entry>
        <!-- Magnetic nord force -->
        <entry key="rab_north_ref_pref" class="class java.lang.String">1</entry>
        <!-- Altitude data, 0 = imperial 1= metric -->
        <entry key="alt_unit_pref" class="class java.lang.String">1</entry>
        <entry key="lastViewedLayer.active" class="class java.lang.String">Mobile</entry>
