        <entry key="dynamicReportingRateMinUnreliable" class="class java.lang.String">20</entry>
        <entry key="dynamicReportingRateMaxUnreliable" class="class java.lang.String">5</entry>
        <entry key="dynamicReportingRateMinReliable" class="class java.lang.String">20</entry>
        <entry key="dynamicReportingRateStationaryUnreliable" class="class java.lang.String">30</entry>
        <entry key="dynamicReportingRateStationaryReliable" class="class java.lang.String">180</entry>
        <entry key="constantReportingRateReliable" class="class java.lang.String">15</entry>
        <entry key="dynamicReportingRateMaxReliable" class="class java.lang.String">5</entry>
        <entry key="locationReportingStrategy" class="class java.lang.String">Dynamic</entry>
        <entry key="constantReportingRateUnreliable" class="class java.lang.String">3</entry>
	    <!-- ADD STALE OUT AND EXPIRATION ON OBJECTS / FIX THESE!! -->		
        <!-- audible alarms -->
        <entry key="alert_notification" class="class java.lang.Boolean">true</entry>
		<!-- Enable user location -->
		<entry key="user_location.enabled" class="class java.lang.Boolean">true</entry>
		<!-- Avoid accidential quit or shutdown of ATAK -->
		<entry key="atakControlQuitOnBack" class="class java.lang.Boolean">false</entry>
		<entry key="atakControlAskToQuit" class="class java.lang.Boolean">true</entry>
    </preference>
</preferences>
